		</action>
		
		<action name="login5" class="com.beyond.demo3.LoginAction5">
			<result type="redirect">/index.jsp</result>
			<result name="input" type="dispatcher">/demo3/login5.jsp</result>
		</action>
		
		<action name="login6" class="com.beyond.demo3.LoginAction6">
			<result type="redirect">/index.jsp</result>
			<result name="input" type="dispatcher">/demo3/login6.jsp</result>
